<Window x:Class="InventoryManager.inventoryInterface"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:InventoryManager"
        mc:Ignorable="d"
        Title="inventoryInterface" Height="700" Width="700">
    <Grid>
        <Label x:Name="label" Content="Enter Locaton" HorizontalAlignment="Left" Margin="57,64,0,0" VerticalAlignment="Top" Width="99"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="23" Margin="182,67,0,0" TextWrapping="Wrap" Text="A101" VerticalAlignment="Top" Width="120" TextChanged="textBox_TextChanged"/>
        <Button x:Name="button" Content="Enter" HorizontalAlignment="Left" Margin="65,276,0,0" VerticalAlignment="Top" Width="75" Click="button_Click"/>
        <ListBox x:Name="listBox" HorizontalAlignment="Left" Height="156" Margin="68,99,0,0" VerticalAlignment="Top" Width="436" SelectionChanged="listBox_SelectionChanged"/>
        <ComboBox x:Name="comboBox" HorizontalAlignment="Left" Margin="352,64,0,0" VerticalAlignment="Top" Width="201" SelectionChanged="comboBox_SelectionChanged"/>

    </Grid>
</Window>
