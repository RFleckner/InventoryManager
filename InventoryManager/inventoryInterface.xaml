        <Button x:Name="button2" Content="Update" HorizontalAlignment="Left" Margin="78,319,0,0" VerticalAlignment="Top" Width="76" Click="button2_Click"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="24" Margin="177,270,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120" TextChanged="textBox_TextChanged_1"/>
        <Label x:Name="label1" Content="update Quanity" HorizontalAlignment="Left" Margin="72,267,0,0" VerticalAlignment="Top" Width="114"/>

    </Grid>
